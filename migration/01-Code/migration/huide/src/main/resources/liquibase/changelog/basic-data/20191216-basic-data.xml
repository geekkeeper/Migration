	<!--人员表(BD_Person)-->
	<changeSet author="WangXun" id="1576425600000-3" >
		<comment>新建人员表(BD_Person)</comment>
		<createTable tableName="BD_PERSON" remarks="人员表">
			<column name="ID" type="${id}" remarks="人员编号">
				<constraints primaryKey="true" primaryKeyName="PK_PERSON_ID"/>
			</column>
			<column name="PERSON_NAME" type="${name}" remarks="人员名称">
				<constraints nullable="false" notNullConstraintName="NU_PERSON_USERNAME" unique="true" uniqueConstraintName="UQ_PERSON_USERNAME"/>
			</column>
			<column name="GENDER" type="char(1)" remarks="性别:M-男,F-女,U-未知"
					defaultValue="U" defaultValueConstraintName="DF_PERSON_GENDER"/>
			<column name="ADDRESS" type="varchar(32)" remarks="住址"/>
			<column name="ORIGIN" type="varchar(32)" remarks="户籍"/>
			<column name="IDENTITY_CARD" type="char(18)" remarks="身份证"/>
			<column name="WORK_NO" type="${work_no}" remarks="工号"/>
			<column name="EMAIL" type="${email}" remarks="邮箱"/>
			<column name="WECHAT" type="${wechat}" remarks="微信"/>
			<column name="PHONE" type="${phone}" remarks="移动电话"/>
			<column name="TELEPHONE" type="varchar(16)" remarks="固定电话"/>
			<column name="BIRTHDAY" type="${datetime}" remarks="生日"/>
			<column name="DELETE_TAG" type="${boolean}" remarks="删除标识:‘Y’-已删除，‘N’-未删除"
					defaultValue="N" defaultValueConstraintName="DF_Person_DELETE_TAG"/>
			<column name="CREATE_TIME" type="${datetime}" remarks="创建时间"/>
			<column name="CREATE_BY" type="${id}" remarks="创建人"/>
			<column name="UPDATE_TIME" type="${datetime}" remarks="修改时间"/>
			<column name="UPDATE_BY" type="${id}" remarks="修改人"/>
		</createTable>
	</changeSet>
	<!--公司表-->
	<changeSet author="Sun YiWen" id="1581661583168-1">
		<createTable remarks="公司表" tableName="BD_COMPANY">
			<column name="ID" remarks="公司编号" type="${id}">
				<constraints primaryKey="true" primaryKeyName="PK_BD_COMPANY"/>
			</column>
			<column name="CODE" remarks="公司编码" type="${code}"/>
			<column name="SHORT_NAME" remarks="公司名称（简称）" type="${name}"/>
			<column name="FULL_NAME" remarks="公司名称（全称）" type="${name}">
				<constraints nullable="false"/>
			</column>
			<column name="STATE" remarks="公司状态" type="${type}"/>
			<column name="TYPE" remarks="公司类型" type="${type}"/>
			<column name="PARENT_ID" remarks="父级编号" type="${id}"/>
			<column name="PARENT_NAME" remarks="父级名称" type="${name}"/>
			<column name="SORT_INDEX" remarks="排序编号:同级排序" type="${sort}"/>
			<column name="SYSTEMATIC" remarks="系统标识" type="${boolean}"/>
			<column name="EXTERNAL_TAG" remarks="外部标识：y-外部的，n-内部的" type="${boolean}"/>
			<column name="DELETE_TAG" remarks="删除标识:N-未删除、Y-已删除" type="${boolean}"/>
			<column name="CREATE_BY" remarks="创建人" type="${id}"/>
			<column name="CREATE_TIME" remarks="创建时间" type="${datetime}"/>
			<column name="UPDATE_BY" remarks="修改人" type="${id}"/>
			<column name="UPDATE_TIME" remarks="修改时间" type="${datetime}"/>
			<column name="REMARK" remarks="备注信息,描述信息" type="${remark}"/>
			<column name="REGIONAL_CODE" remarks="区域编号" type="${code}"/>
			<column name="ADDRESS" remarks="详细地址" type="${address}"/>
			<column name="LEVEL" remarks="层级编号" type="${level}"/>
			<column name="EMAIL" remarks="邮箱" type="${email}"/>
			<column name="PHONE" remarks="联系电话" type="${phone}"/>
		</createTable>
	</changeSet>
