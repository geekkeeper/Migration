                <constraints nullable="false"/>
            </column>
            <column name="PROJECT_ID" type="${id}" remarks="项目编号">
                <constraints nullable="false"/>
            </column>
            <column name="GT_PROJECT_GUID" type="${code}" remarks="GT工程编码">
                <constraints nullable="false"/>
            </column>
