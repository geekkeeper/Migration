    <!--行政区划表-->
    <changeSet author="WangXun" id="1576425600000-6">
        <comment>创建行政区划表(BD_REGIONAL_DIVISION)</comment>
        <createTable tableName="BD_REGIONAL_DIVISION" remarks="行政区划表">
            <column name="REGIONAL_ID" remarks="区域编号" type="${id}">
                <constraints primaryKey="true" primaryKeyName="PK_BD_REGIONAL_DIVISION"/>
            </column>
            <column name="CODE" remarks="区域编码" type="${code}">
                <constraints nullable="false"/>
            </column>
            <column name="NAME" remarks="区域名称" type="${name}">
                <constraints nullable="false"/>
            </column>
            <column name="FULL_NAME" remarks="区域全名" type="${name}">
                <constraints nullable="false"/>
            </column>
            <column name="TYPE" remarks="区域类型" type="${type}"/>
            <column name="PARENT_NAME" remarks="父级名称" type="${name}"/>
            <column name="PARENT_ID" remarks="父级编号" type="${id}"/>
            <column name="LEVEL" remarks="层级编号" type="${level}"/>
            <column name="SORT_INDEX" remarks="排序编号:同级排序" type="${sort}"/>
            <column name="DELETE_TAG" remarks="删除标识:N-未删除、Y-已删除" type="${boolean}"/>
            <column name="CREATE_BY" remarks="创建人" type="${id}"/>
            <column name="CREATE_TIME" remarks="创建时间" type="${datetime}"/>
            <column name="UPDATE_BY" remarks="修改人" type="${id}"/>
            <column name="UPDATE_TIME" remarks="修改时间" type="${datetime}"/>
            <column name="REMARK" remarks="备注信息,描述信息" type="${remark}"/>
        </createTable>
    </changeSet>
