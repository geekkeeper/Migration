    <!--人员表-->
    <changeSet author="WangXun" id="1576425600000-3">
        <comment>新建人员表(BD_Person)</comment>
        <createTable tableName="BD_PERSON" remarks="人员表">
            <column name="ID" type="${id}" remarks="人员编号">
                <constraints primaryKey="true" primaryKeyName="PK_PERSON_ID"/>
            </column>
            <column name="PERSON_NAME" type="${name}" remarks="人员名称">
                <constraints nullable="false" notNullConstraintName="NU_PERSON_USERNAME" unique="true"
                             uniqueConstraintName="UQ_PERSON_USERNAME"/>
            </column>
            <column name="GENDER" type="char(1)" remarks="性别:M-男,F-女,U-未知"
                    defaultValue="U" defaultValueConstraintName="DF_PERSON_GENDER"/>
            <column name="ADDRESS" type="varchar(32)" remarks="住址"/>
            <column name="ORIGIN" type="varchar(32)" remarks="户籍"/>
            <column name="IDENTITY_CARD" type="char(18)" remarks="身份证"/>
            <column name="WORK_NO" type="${work_no}" remarks="工号"/>
            <column name="EMAIL" type="${email}" remarks="邮箱"/>
            <column name="WECHAT" type="${wechat}" remarks="微信"/>
            <column name="PHONE" type="${phone}" remarks="移动电话"/>
            <column name="TELEPHONE" type="varchar(16)" remarks="固定电话"/>
            <column name="BIRTHDAY" type="${datetime}" remarks="生日"/>
            <column name="BIND_TAG" type="${boolean}" remarks="绑定标识"/>
            <column name="DELETE_TAG" type="${boolean}" remarks="删除标识:‘Y’-已删除，‘N’-未删除"
                    defaultValue="N" defaultValueConstraintName="DF_Person_DELETE_TAG"/>
            <column name="CREATE_TIME" type="${datetime}" remarks="创建时间"/>
            <column name="CREATE_BY" type="${id}" remarks="创建人"/>
            <column name="UPDATE_TIME" type="${datetime}" remarks="修改时间"/>
            <column name="UPDATE_BY" type="${id}" remarks="修改人"/>
        </createTable>
    </changeSet>
    <!--组织机构表-->
    <changeSet author="WangXun" id="1576425600000-4">
        <comment>创建组织机构表(BD_ORGANIZATION)</comment>
        <createTable tableName="BD_ORGANIZATION" remarks="组织机构表">
            <column name="ID" type="${id}" remarks="组织机构编号">
                <constraints primaryKey="true" primaryKeyName="PK_ORGANIZATION_ID"/>
            </column>
            <column name="CODE" type="${code}" remarks="组织机构编码"/>
            <column name="SHORT_NAME" type="${name}" remarks="组织机构名称（简称）"/>
            <column name="FULL_NAME" type="${name}" remarks="组织机构名称（全称）"/>
            <column name="ICON" type="${url}" remarks="组织机构图标"/>
            <column name="STATE" type="${type}" remarks="组织机构状态"/>
            <column name="TYPE" type="${type}" remarks="组织机构类型"/>
            <column name="PARENT_ID" type="${id}" remarks="父级编号"/>
            <column name="LEVEL" type="${level}" remarks="层级编号"/>
            <column name="SORT_INDEX" type="${sort}" remarks="排序编号"/>
            <column name="SYSTEMATIC" type="${boolean}" remarks="系统标识:Y-系统的;N-自定义的"/>
            <column name="EXTERNAL_TAG" type="${boolean}" remarks="外部标识：y-外部的，n-内部的"/>
            <column name="DELETE_TAG" type="${boolean}" remarks="删除标识:N-未删除、Y-已删除"/>
            <column name="CREATE_BY" type="${id}" remarks="创建人"/>
            <column name="CREATE_TIME" type="${datetime}" remarks="创建时间"/>
            <column name="UPDATE_BY" type="${id}" remarks="修改人"/>
            <column name="UPDATE_TIME" type="${datetime}" remarks="修改时间"/>
            <column name="REMARK" type="${remark}" remarks="备注信息,描述信息"/>
        </createTable>
