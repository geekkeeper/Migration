<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC
		"-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!--引入属性文件：JDBC配置-->
<!--	<properties resource="jdbc-mysql.properties"/>-->
	<!--本地数据库驱动文件-->
	<classPathEntry location="D:\Development\Apache\Maven\Repository\mysql\mysql-connector-java\8.0.18\mysql-connector-java-8.0.18.jar"/>

	<!--MyBatis上下文-->
	<context id="context" targetRuntime="MyBatis3Simple">
		<!--配置注释生成器的属性-->
		<commentGenerator>
			<!--取消所有注释-->
			<property name="suppressAllComments" value="false"/>
			<!---->
			<property name="suppressDate" value="false"/>
			<!--添加时间格式化模式-->
			<property name="dateFormat" value="yyyy-MM-dd HH:mm:ss"/>
			<!--添加数据库中的注释-->
			<property name="addRemarkComments" value="true"/>
		</commentGenerator>
		<!--配置JDBC连接数据-->
		<jdbcConnection userId="root"
						password="HiODS-9527"
						driverClass="com.mysql.cj.jdbc.Driver"
						connectionURL="jdbc:mysql://172.21.50.60:3306/integration?serverTimezone=GMT%2B8"/>

		<!--配置java类型解析器-->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false"/>
		</javaTypeResolver>

		<!--配置生成实体存在路径-->
		<javaModelGenerator targetPackage="com.xunsoft.integration.entity" targetProject=".">
			<property name="enableSubPackages" value="false"/>
			<!--字符串删除两端空格-->
			<property name="trimStrings" value="true"/>
		</javaModelGenerator>

		<!--配置Mapper XML存放路径-->
		<sqlMapGenerator targetPackage="MyBatis/Mapper" targetProject=".">
			<property name="enableSubPackages" value="false"/>
		</sqlMapGenerator>

		<!--配置DAO存放路径-->
		<javaClientGenerator targetPackage="com.xunsoft.integration.dao" type="XMLMAPPER" targetProject=".">
			<property name="enableSubPackages" value="false"/>
		</javaClientGenerator>

		<table schema="integration" tableName="department" enableCountByExample="false" enableDeleteByExample="false"
			   enableSelectByExample="false" enableUpdateByExample="false"/>
	</context>
</generatorConfiguration>